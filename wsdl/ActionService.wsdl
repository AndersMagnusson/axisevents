      <!--===== Types Definitions =======-->

      <xs:complexType name="NewActionRule">
        <xs:sequence>
          <xs:element name="Name" type="xs:string" minOccurs="0">
            <xs:annotation>
              <xs:documentation>
                User defined name for the action rule.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Enabled" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>
                Enable the rule. If false, the rule will be stored in idle state and
                action(s) will not be activated.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="StartEvent" type="wsnt:FilterType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>
                Start event. If specified, the action will be
                executed when the start event is emitted and all conditions, if any,
                are fulfilled.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Conditions"
                      type="aa:Conditions"
                      minOccurs="0"
                      maxOccurs="1">
            <xs:annotation>
              <xs:documentation>
			    List of conditions that must be fulfilled for the action to
				be executed. The order of the conditions will be preserved in
				GetActionRules requests.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ActivationTimeout"
                      type="xs:duration"
                      minOccurs="0"
                      maxOccurs="1">
            <xs:annotation>
              <xs:documentation>
				Minimum time between successive action executions. This is used to prevent
				the action rule from being triggered repeatedly. If not
                specified, action will be executed as soon as possible.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="PrimaryAction" type="xs:integer">
            <xs:annotation>
              <xs:documentation>
                The primary action to be executed, specified by its Action Configuration ID.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="FailoverAction"
                      type="xs:integer"
                      minOccurs="0"
                      maxOccurs="1">
            <xs:annotation>
              <xs:documentation>
                Action Configuration ID for the fallback action.
				The fallback action will be executed if the primary action fails.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
